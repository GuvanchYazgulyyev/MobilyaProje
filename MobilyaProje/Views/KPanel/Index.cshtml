
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<!-- Notice -->
<div class="row">
    <div class="col-md-12">
        <div class="notification success closeable margin-bottom-30">
            <h4>Hoş Geldiniz: @ViewBag.adsoyad!</h4>
            <a class="close" href="#"></a>
        </div>
    </div>
</div>

<!-- Content -->
<div class="row">

    <!-- Item -->
    <div class="col-lg-3 col-md-6">
        <div class="dashboard-stat color-1">
            <div class="dashboard-stat-content"><h4>@ViewBag.d2</h4> <span> Hizmetler </span></div>
            <div class="dashboard-stat-icon"><i class="im im-icon-Map2"></i></div>
        </div>
    </div>

    <!-- Item -->
    <div class="col-lg-3 col-md-6">
        <div class="dashboard-stat color-2">
            <div class="dashboard-stat-content"><h4>@ViewBag.d3</h4> <span>Müşteri Yorumları</span></div>
            <div class="dashboard-stat-icon"><i class="im im-icon-Line-Chart"></i></div>
        </div>
    </div>


    <!-- Item -->
    <div class="col-lg-3 col-md-6">
        <div class="dashboard-stat color-3">
            <div class="dashboard-stat-content"><h4>@ViewBag.d4</h4> <span>Bloglar</span></div>
            <div class="dashboard-stat-icon"><i class="im im-icon-Add-UserStar"></i></div>
        </div>
    </div>

    <!-- Item -->
    <div class="col-lg-3 col-md-6">
        <div class="dashboard-stat color-4">
            <div class="dashboard-stat-content"><h4>@ViewBag.d5</h4> <span>Mesajlar</span></div>
            <div class="dashboard-stat-icon"><i class="im im-icon-Heart"></i></div>
        </div>
    </div>
</div>



